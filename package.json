{
  "name": "sviewer",
  "version": "0.0.1",
  "description": "Webcomponent for drawing glycans",
  "main": "./js/index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "debug-any-level": "^0.3.0"
  },
  "devDependencies": {
    "DragMenus": "hirenj/DragMenus",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.0",
    "glycan.js": "hirenj/glycan.js",
    "jshint": "^2.8.0",
    "precommit-hook": "^3.0.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  },
  "scripts": {
    "jshint-pre-commit": "./scripts/jshint-pre-commit.sh",
    "build": "./node_modules/babel-cli/bin/babel.js js -d dist",
    "build-watch": "./node_modules/babel-cli/bin/babel.js lib js --watch -d dist",
